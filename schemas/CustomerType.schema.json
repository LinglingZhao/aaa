{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "CustomerType",
  "description": "客户类型实体",
  "properties": {
    "id": {
      "type": "string",
      "description": "主键ID",
      "maxLength": 36
    },
    "orderNum": {
      "type": "integer",
      "description": "序号"
    },
    "fullUnitName": {
      "type": "string",
      "description": "单位全称",
      "maxLength": 256
    },
    "customerType": {
      "type": "string",
      "description": "客户类型",
      "maxLength": 256
    },
    "modelType": {
      "type": "string",
      "description": "型号类型",
      "maxLength": 256
    },
    "radarChart": {
      "type": "string",
      "description": "雷达图",
      "maxLength": 256
    }
  },
  "required": ["id", "orderNum", "fullUnitName"]
}