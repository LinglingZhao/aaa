{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "CustomerPowerNeeds",
  "description": "客户用电需求实体",
  "properties": {
    "id": {
      "type": "integer",
      "description": "主键ID"
    },
    "orderNum": {
      "type": "integer",
      "description": "序号"
    },
    "fullUnitName": {
      "type": "string",
      "description": "单位全称",
      "maxLength": 256
    },
    "doesRequirePower": {
      "type": "string",
      "description": "动力是否需求",
      "maxLength": 256
    },
    "powerType": {
      "type": "string",
      "description": "动力类型",
      "maxLength": 256
    },
    "technicalRequisitionOfPower": {
      "type": "string",
      "description": "动力技术需求",
      "maxLength": 2560
    },
    "demandOfPowerQuantity": {
      "type": "string",
      "description": "动力市场需求量 (台)",
      "maxLength": 256
    }
  },
  "required": ["id", "orderNum", "fullUnitName"]
}